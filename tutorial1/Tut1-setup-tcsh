#!/bin/tcsh

    setenv TOSROOT "/opt/tinyos-2.1.0";
    setenv TOSDIR "$TOSROOT/tos";
    #this assumes that there is no other CLASSPATH set.
    #setenv CLASSPATH "{$CLASSPATH}:{$TOSROOT}/support/sdk/java"
    setenv CLASSPATH ".:{$TOSROOT}/support/sdk/java";
    setenv MAKERULES "$TOSROOT/support/make/Makerules";
    echo "Setting up for TinyOS 2.1.0 in a tcsh shell";
